<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
    <info organisation="lexevs" module="lbIndex" revision="${vBuild}"/>
    <configurations>
		<include file="${lbPackager.dir}/ivyconfigurations.xml"/>
	</configurations>
	<publications>
		<artifact name="lbIndex" type="pom" ext="pom" conf="pom"/>
		<artifact name="lbIndex" type="jar" ext="jar" conf="default"/>
		<artifact name="lbIndex" type="source" ext="jar"
			 m:classifier="sources" conf="sources" />
		<artifact name="lbIndex" type="test" ext="jar"
			 m:classifier="tests" conf="test"/>
	</publications>
	<dependencies>
		<dependency org="org.apache.lucene" name="lucene-core" rev="4.10.2" force="true" conf="default"/>
		<dependency org="org.apache.lucene" name="lucene-analyzers-common" rev="4.10.2" force="true" conf="default"/>
		<dependency org="org.apache.lucene" name="lucene-queries" rev="4.10.2" force="true" conf="default"/>
		<dependency org="org.apache.lucene" name="lucene-queryparser" rev="4.10.2" force="true" conf="default"/>
   	 </dependencies>
</ivy-module>